{
  "name": "agent-eval",
  "version": "1.0.0",
  "description": "AgentEval - A framework for evaluating AI agent execution traces",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "start:mock": "tsx src/mock-server.ts",
    "agent": "npm run build && node dist/agentRunner.js",
    "start": "npm run build && node dist/agentRunner.js",
    "evaluate": "npm run build && node dist/evaluate.js",
    "test": "npm run build && ./test.sh",
    "dev:mock": "tsx watch src/mock-server.ts",
    "lint": "tsc --noEmit"
  },
  "keywords": [
    "agent",
    "evaluation",
    "testing",
    "ai",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/agent-eval.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/agent-eval/issues"
  },
  "homepage": "https://github.com/yourusername/agent-eval#readme",
  "dependencies": {
    "ajv": "^8.12.0",
    "dotenv": "^17.2.3",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
